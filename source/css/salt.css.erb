body{background: #d4d4d4 url("../img/bg.png");font-family: "Helvetica Neue", "Segoe UI", "Roboto", Helvetica, Arial, sans-serif;margin:0;padding:0;text-align:center;}
h1 a{color:#000!important;text-decoration:none;font-weight:200;}
h2 a{font-weight:200;}
h1 a img{vertical-align: middle;margin:0.5em 0;}
h1{margin:0;font-size:3em;}
h2{padding:0.5em 0;font-size: 32px;margin: 0.83em 0;}
h1,h2,h3,h4,h5,h6{font-weight:300;text-decoration:none;line-height:1.1;}
p{margin:1.5em 0;line-height: 1.9em;}
.center{display:block;margin:0.5em auto;}
.content{padding:1em;margin:0 auto 0 auto;text-align:left;max-width:800px;background:#fff;}
pre.highlight{overflow:auto;}
.footer,.header{padding:1em;margin:0 auto 0 auto;max-width:800px;background:#bbdefb;}
.footer ul {list-style-type:none;text-align:center;margin:0;padding:0;}
.footer ul li{display:inline;padding:0 0.2em;}
a{font-weight:bold;}
a:link{color:#009;}
a:visited{color:#909}
a:active,a:hover,a:focus{color:#f00}
.video-container{position:relative;padding-bottom:56.25%;padding-top:35px;height:0;overflow:hidden;margin:0.5em auto;}
.video-container iframe{position:absolute;top:0;left:0;width:100%;height:100%;}
<%= Rouge::Themes::Github.render(:scope => '.highlight') %>
